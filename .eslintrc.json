{
  "extends": [
    "airbnb-base"
  ],
  "globals": {
    "Chart": true
  },
  "rules": {
    "func-names": "off",
    "no-console": "off",
    "no-plusplus": "off",
    "no-unused-expressions": "off",
    "no-alert": "off",
    "no-param-reassign": "off",
    "no-return-assign": "off",
    "no-prototype-builtins": "off",
    "no-restricted-syntax": "off",
    "no-extend-native": "off",
    "arrow-parens": "off",
    "arrow-body-style": "off",
    "prefer-arrow-callback": "off",
    "no-loop-func": "off"
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true,
    "mocha": true
  }
}
